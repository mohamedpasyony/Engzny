{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/css/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css"
      ]
    },
    {
      "library": "bootbox.js@6.0.0",
      "destination": "wwwroot/lib/bootbox.js/",
      "files": [
        "bootbox.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}